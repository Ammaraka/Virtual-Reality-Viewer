	<widgetset name="web">
		<label>
			Web Content
		</label>
		<icon>
			icn_media_web.tga
		</icon>
		<default_type>
			text/html
		</default_type>
		<tooltip>
			This location has Web content
		</tooltip>
		<playtip>
			Show Web content
		</playtip>
		<allow_resize>
			true
		</allow_resize>
		<allow_looping>
			false
		</allow_looping>
	</widgetset>
